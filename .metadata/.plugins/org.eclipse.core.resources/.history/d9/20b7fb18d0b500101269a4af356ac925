package ej;


public class Bloc3 {
	private int longueur;
	private int largeur;
	private int hauteur;
	
    public String getAttributs() {
        return this.longueur;
        return this.largeur;
        return this.hauteur;
    }
    
    public void setAttributs(int longueur, int largeur, int hauteur) {
        this.longueur = longueur;
        this.largeur = largeur;
        this.hauteur = hauteur;
    }
	

}