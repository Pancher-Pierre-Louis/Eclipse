package ej;


public class Bloc4 {
	
	protected int longueur;
	protected int largeur;
	protected int hauteur;
	
    public Bloc4(final int longueur, final int largeur, final int hauteur) {
        this.longueur = longueur;
        this.largeur = largeur;
        this.hauteur = hauteur;
    }
    
 
}